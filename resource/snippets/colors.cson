'.source.scss':
  'Colorful | setRobot':
    'prefix': 'colors'
    'body': '''
    $${1:main-color}: ${2:#f90};
    $${1:main-color}-lightest: lighten($${1:main-color}, 30%);
    $${1:main-color}-lighter: lighten($${1:main-color}, 20%);
    $${1:main-color}-light: lighten($${1:main-color}, 10%);
    $${1:main-color}-dark: darken($${1:main-color}, 10%);
    $${1:main-color}-darker: darken($${1:main-color}, 20%);
    $${1:main-color}-darkest: darken($${1:main-color}, 30%);
'''
